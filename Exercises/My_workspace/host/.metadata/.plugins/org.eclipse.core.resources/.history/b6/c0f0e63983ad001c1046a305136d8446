#include <stdio.h>


int main ()
{

// nhập 2 số A.B là đầu cuối của một mạng
	int A,B,soLuong;
	 printf("nhap so A\n");
	 fflush(stdout);
	 scanf("%d",&A);
	 printf("nhap so B\n");
	 fflush(stdout);
	 scanf("%d",&B);
	 soLuong = (A - B)+1;
	 int arr[soLuong];

	 // tạo mảng
	 for(int i=0;i<soLuong;i++)
	 {
		 arr[i] = A +i;
		 printf('arr[%d] = %d',i,arr[i]);
		 fflush(stdout);
	 }






	 printf("Press enter key to exit this application");

	while(getchar() != '\n')
	{
	//just read the input buffer and do nothing
	 }
     getchar();
}
