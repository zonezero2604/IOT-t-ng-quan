#include <stdio.h>




int main() {


  int num,kiemTra;
  do {
     printf("nhap input \n ");
     fflush(stdout);
     kiemTra = scanf("%d",&num);
     if (kiemTra < 1) {    // Clean the input
       while (getchar() != '\n') ;
     }
  } while(kiemTra < 1 && num>0);

  for(int i=1; i<=num; i++)
  {
	  if(i%2==0)
	  {
		  printf("%d ",i);
		        fflush(stdout);
	  }

  }

	 while(getchar() != '\n')
	 {
	//just read the input buffer and do nothing
	 }
 getchar();
   return 0;
}
